 <%if agency_signed_in? %>

<%= render 'layouts/navbar' %>
<%= render 'layouts/property_show_visitors', property: @property %>

<h5 class="card-title text-blue text-center mt-10"><%= @property.title %></h5>

<%= render 'layouts/property_show_carroussel', property: @property %>
<%= render 'layouts/property_show_description', property: @property%>
<%= render 'layouts/footer' %>

<% else %>

<%= render 'layouts/navbar' %>
<%= render 'layouts/property_show_photo_description' %>
<%= render 'layouts/property_show_description', property: @property%>
<%= render 'layouts/property_show_carroussel' %>

<%= render 'layouts/footer' %>

<% end %>
